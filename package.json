    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "sass": "^1.90.0",
    "sass-embedded": "^1.89.2",
    "sass-loader": "^16.0.5",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.39.0",
