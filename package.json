    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.2.4",
    "@iconify-json/fa7-solid": "^1.2.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/compiler-sfc": "^3.5.18",
