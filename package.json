    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "bootstrap": "^5.3.7",
    "bootstrap-vue-next": "^0.30.4",
    "moment": "^2.30.1",
